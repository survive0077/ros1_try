<launch>
    <arg name="ns" default="robot1"/>
    <arg name="use_gui" default="true"/>
    <arg name="robot_type" value="X3"/>
    <param name="robot_car_gazebo" command="$(find xacro)/xacro --inorder
        '$(find yahboom_car_gazebo)/urdf/yahboomcar_$(arg robot_type).urdf.xacro' ns:=$(arg ns)"/>
<!--    <param name="robot_car_gazebo" command="$(find xacro)/xacro &#45;&#45;inorder-->
<!--        '$(find yahboom_car_gazebo)/urdf/yahboomcar_$(arg robot_type).urdf.xacro' ns:=$(arg ns)" />-->
    <node name="joint_state_publisher" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui"
          if="$(arg use_gui)"/>
    <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher"
          unless="$(arg use_gui)"/>
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find yahboom_car_gazebo)/rviz/yahboomcar.rviz"/>
</launch>