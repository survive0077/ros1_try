<launch>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <rosparam file="$(find yahboom_car_gazebo)/param/costmap_common_params_X3.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find yahboom_car_gazebo)/param/costmap_common_params_X3.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find yahboom_car_gazebo)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find yahboom_car_gazebo)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find yahboom_car_gazebo)/param/dwa_local_planner_params_X3.yaml" command="load" />
    </node>
</launch>
